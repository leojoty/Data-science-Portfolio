# Very simple structure:
#plot(customer, verbose = T)

set.seed(123)
envs <- lapply(1:200, function(i) {
simmer("Bank") %>%
 add_resource("counter", 3) %>%
 add_generator("customer", customer, function() rexp(1, 1/4)) %>%
 run(360)
})
